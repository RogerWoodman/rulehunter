{
  "title": "What would indicate good flow?",
  "tags": ["test", "fred / ned"],
  "dataset": "csv",
  "csv": {
    "filename": "fixtures/flow.csv",
    "hasHeader": true,
    "separator":  ","
  },
  "fieldNames": ["group","district","height","flow"],
  "ruleFieldNames": ["group","district","height"],
  "aggregators": [
    {
      "name": "goodFlowMcc",
      "function": "mcc",
      "arg": "flow > 60"
    }
  ],
  "goals": ["goodFlowMcc > 0"],
  "sortOrder": [
    {
      "aggregatorName": "goodFlowMcc",
      "direction": "descending"
    },
    {
      "aggregatorName": "numMatches",
      "direction": "descending"
    }
  ]
}
