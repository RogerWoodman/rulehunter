{
  "title": "What would indicate good flow?",
  "tags": ["test", "fred / ned"],
  "dataset": "csv",
  "csv": {
    "filename": "fixtures/flow.csv",
    "hasHeader": true,
    "separator":  ","
  },
  "fieldNames": ["group","district","height","flow"],
  "excludeFieldNames": ["flow"],
  "aggregators": [
    {
      "name": "goodFlowAccuracy",
      "function": "accuracy",
      "arg": "flow > 60"
    }
  ],
  "goals": ["goodFlowAccuracy > 10"],
  "sortOrder": [
    {
      "aggregatorName": "goodFlowAccuracy",
      "direction": "descending"
    },
    {
      "aggregatorName": "numMatches",
      "direction": "descending"
    }
  ]
}
